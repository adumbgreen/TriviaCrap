<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppBaseTheme" parent="@style/Theme.AppCompat.Light" />
    <style name="Theme.Chat" parent="@style/Theme.AppCompat.Light">
        <item name="android:actionBarStyle">@style/MyActionBar</item>
        <item name="android:titleTextStyle">@style/ActionBar.Title</item>
        <item name="android:subtitleTextStyle">@style/ActionBar.Subtitle</item>
    </style>
    <style name="MyActionBar" parent="@style/Widget.AppCompat.ActionBar">
        <item name="android:background">@color/aqua</item>
        <item name="android:height">50.0dip</item>
        <item name="android:titleTextStyle">@style/ActionBar.Title</item>
        <item name="android:subtitleTextStyle">@style/ActionBar.Subtitle</item>
    </style>
    <style name="Widget.AppCompat.Base.ProgressBar.Horizontal" parent="@*android:style/Widget.Holo.ProgressBar.Horizontal" />
    <style name="Widget.AppCompat.Base.ProgressBar" parent="@*android:style/Widget.Holo.ProgressBar" />
    <style name="Widget.AppCompat.Base.AutoCompleteTextView" parent="@*android:style/Widget.Holo.AutoCompleteTextView" />
    <style name="Widget.AppCompat.Light.Base.AutoCompleteTextView" parent="@*android:style/Widget.Holo.Light.AutoCompleteTextView" />
    <style name="Theme.Base" parent="@*android:style/Theme.Holo">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowActionModeOverlay">true</item>
        <item name="buttonBarStyle">?android:buttonBarStyle</item>
        <item name="buttonBarButtonStyle">?android:buttonBarButtonStyle</item>
        <item name="selectableItemBackground">?android:selectableItemBackground</item>
    </style>
    <style name="Theme.Base.Light" parent="@*android:style/Theme.Holo.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowActionModeOverlay">true</item>
        <item name="buttonBarStyle">?android:buttonBarStyle</item>
        <item name="buttonBarButtonStyle">?android:buttonBarButtonStyle</item>
        <item name="selectableItemBackground">?android:selectableItemBackground</item>
    </style>
    <style name="Theme.Base.AppCompat.Dialog.FixedSize" parent="@*android:style/Theme.Holo.Dialog">
        <item name="windowActionBar">false</item>
        <item name="windowFixedWidthMajor">@dimen/dialog_fixed_width_major</item>
        <item name="windowFixedHeightMinor">@dimen/dialog_fixed_height_minor</item>
        <item name="windowFixedWidthMinor">@dimen/dialog_fixed_width_minor</item>
        <item name="windowFixedHeightMajor">@dimen/dialog_fixed_height_major</item>
        <item name="buttonBarStyle">?android:buttonBarStyle</item>
        <item name="buttonBarButtonStyle">?android:buttonBarButtonStyle</item>
        <item name="selectableItemBackground">?android:selectableItemBackground</item>
    </style>
    <style name="Theme.Base.AppCompat.Dialog.Light.FixedSize" parent="@*android:style/Theme.Holo.Light.Dialog">
        <item name="windowActionBar">false</item>
        <item name="windowFixedWidthMajor">@dimen/dialog_fixed_width_major</item>
        <item name="windowFixedHeightMinor">@dimen/dialog_fixed_height_minor</item>
        <item name="windowFixedWidthMinor">@dimen/dialog_fixed_width_minor</item>
        <item name="windowFixedHeightMajor">@dimen/dialog_fixed_height_major</item>
        <item name="buttonBarStyle">?android:buttonBarStyle</item>
        <item name="buttonBarButtonStyle">?android:buttonBarButtonStyle</item>
        <item name="selectableItemBackground">?android:selectableItemBackground</item>
    </style>
    <style name="dialog_color_pciker_view_portrait">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="ProgressBarTheme" parent="@*android:style/Widget.Holo.ProgressBar" />
    <style name="ProgressBarTheme.Small" parent="@*android:style/Widget.Holo.ProgressBar.Small" />
    <style name="ProgressBarTheme.Large" parent="@*android:style/Widget.Holo.ProgressBar.Large" />
    <style name="Theme.BigPictureDialog" parent="@*android:style/Theme.Holo.Light">
        <item name="android:windowIsFloating">false</item>
    </style>
    <style name="DatePickerTheme" parent="@*android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowBackground">@*android:color/transparent</item>
    </style>
    <style name="TvShowRegistrationTheme" parent="@style/Theme.AppCompat.Light">
        <item name="android:windowNoTitle">true</item>
    </style>
</resources>
